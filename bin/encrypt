#!/usr/bin/env bash
set -eo pipefail

KEY=$1

bin/encrypt-file $KEY ssh/config.d/desktop-1
bin/encrypt-file $KEY ssh/config.d/laptop-2
bin/encrypt-file $KEY ssh/config.d/laptop-3
bin/encrypt-file $KEY ssh/config.d/server-1
bin/encrypt-file $KEY ssh/config.d/server-2
bin/encrypt-file $KEY ssh/config.d/nas-1
bin/encrypt-file $KEY ssh/config.d/github.com
